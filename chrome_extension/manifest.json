{
    "manifest_version": 3,
    "version": "1.0",
    "name": "test",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "notifications",
        "scripting",
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "http://api.renzen.io/*",
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ]
        }
    ]
}