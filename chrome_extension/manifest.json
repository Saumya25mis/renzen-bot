{
    "manifest_version": 3,
    "version": "1.0",
    "name": "renzen",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
      },
    "permissions": [
        "tabs",
        "notifications",
        "scripting",
        "activeTab",
        "storage",
        "contextMenus"
    ],
    "host_permissions": [
        "http://api.renzen.io/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ]
        }
    ]
}
