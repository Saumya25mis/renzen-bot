{
    "manifest_version": 3,
    "version": "1.0",
    "name": "test",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "notifications",
        "scripting", 
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        // "https://www.example.com/*"
        // "a.renzen.io/*"
        "http://myLoadBalancerSite-2024255806.us-west-1.elb.amazonaws.com/*"
    ],
    "content_scripts": [
    {   "matches": ["<all_urls>"],
        "js" : ["index.js"]}
    ]
}